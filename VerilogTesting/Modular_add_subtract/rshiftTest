T_0.0 ;
    %load/v 8, v0xec81f0_0, 32;
    %load/v 40, v0xec8340_0, 32;
    %cmp/s 8, 40, 32;
    %jmp/0xz T_0.1, 5;
    %load/v 8, v0xec81f0_0, 32;
    %load/v 40, v0xec8150_0, 4;
    %movi 44, 0, 28;
    %add 8, 40, 32;
    %ix/get 1, 8, 32;
    %jmp/1 T_0.2, 4;
    %load/x1p 8, v0xec8290_0, 1;
    %jmp T_0.3;
T_0.2 ;
    %mov 8, 2, 1;
T_0.3 ;
; Save base=8 wid=1 in lookaside.
    %ix/getv/s 0, v0xec81f0_0;
    %jmp/1 t_0, 4;
    %set/x0 v0xec83e0_0, 8, 1;
t_0 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0xec81f0_0, 32;
    %set/v v0xec81f0_0, 8, 32;
    %jmp T_0.0;
T_0.1 ;
    %set/v v0xec81f0_0, 0, 32;
T_0.4 ;
    %load/v 8, v0xec81f0_0, 32;
    %load/v 40, v0xec8150_0, 4;
    %mov 44, 0, 28;
    %addi 40, 1, 32;
    %cmp/u 8, 40, 32;
    %jmp/0xz T_0.5, 5;
    %load/v 8, v0xec8340_0, 32;
    %load/v 40, v0xec81f0_0, 32;
    %sub 8, 40, 32;
    %ix/get/s 0, 8, 32;
    %jmp/1 t_1, 4;
    %set/x0 v0xec83e0_0, 0, 1;
t_1 ;
    %ix/load 0, 1, 0;
    %load/vp0/s 8, v0xec81f0_0, 32;
    %set/v v0xec81f0_0, 8, 32;
    %jmp T_0.4;
T_0.5 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0xeb3fe0;
T_1 ;
    %set/v v0xec84c0_0, 1, 1;
    %movi 8, 2, 4;
    %set/v v0xec8560_0, 8, 4;
    %vpi_call 2 11 "$display", "Input\011Output";
    %vpi_call 2 12 "$monitor", "%b\011%b", v0xec86c0_0, v0xec8610_0;
