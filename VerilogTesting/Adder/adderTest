#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0xd36fe0 .scope module, "adder_test" "adder_test" 2 2;
 .timescale 0 0;
v0xd499d0_0 .var "ain", 3 0;
v0xd49aa0_0 .var "bin", 3 0;
v0xd49b50_0 .var "clock", 0 0;
v0xd49c00_0 .net "out", 3 0, v0xd49920_0; 1 drivers
S_0xd370d0 .scope module, "testingAdder" "adder" 2 26, 3 1, S_0xd36fe0;
 .timescale 0 0;
v0xd383e0_0 .net "ain", 3 0, v0xd499d0_0; 1 drivers
v0xd497e0_0 .net "bin", 3 0, v0xd49aa0_0; 1 drivers
v0xd49880_0 .net "clock", 0 0, v0xd49b50_0; 1 drivers
v0xd49920_0 .var "out", 3 0;
E_0xd146f0 .event posedge, v0xd49880_0;
S_0xd382f0 .scope begin, "ADD" "ADD" 3 21, 3 21, S_0xd370d0;
 .timescale 0 0;
    .scope S_0xd370d0;
T_0 ;
    %wait E_0xd146f0;
    %fork t_1, S_0xd382f0;
    %jmp t_0;
    .scope S_0xd382f0;
t_1 ;
    %load/v 8, v0xd383e0_0, 4;
    %load/v 12, v0xd497e0_0, 4;
    %add 8, 12, 4;
    %ix/load 0, 4, 0;
    %assign/v0 v0xd49920_0, 0, 8;
    %end;
    .scope S_0xd370d0;
t_0 %join;
    %jmp T_0;
    .thread T_0;
    .scope S_0xd36fe0;
T_1 ;
    %set/v v0xd49b50_0, 1, 1;
    %vpi_call 2 10 "$display", "A value\011B value\011Output";
    %vpi_call 2 11 "$monitor", "%b\011%b\011%b", v0xd499d0_0, v0xd49aa0_0, v0xd49c00_0;
    %set/v v0xd499d0_0, 0, 4;
    %set/v v0xd49aa0_0, 0, 4;
    %delay 2, 0;
    %movi 8, 1, 4;
    %set/v v0xd499d0_0, 8, 4;
    %delay 4, 0;
    %movi 8, 1, 4;
    %set/v v0xd49aa0_0, 8, 4;
    %delay 6, 0;
    %movi 8, 3, 4;
    %set/v v0xd499d0_0, 8, 4;
    %delay 8, 0;
    %movi 8, 7, 4;
    %set/v v0xd49aa0_0, 8, 4;
    %delay 10, 0;
    %movi 8, 9, 4;
    %set/v v0xd499d0_0, 8, 4;
    %vpi_call 2 19 "$finish";
    %end;
    .thread T_1;
    .scope S_0xd36fe0;
T_2 ;
    %delay 1, 0;
    %load/v 8, v0xd49b50_0, 1;
    %inv 8, 1;
    %set/v v0xd49b50_0, 8, 1;
    %jmp T_2;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "./adderTest.v";
    "./adder.v";
